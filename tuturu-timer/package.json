{
  "name": "tuturu-timer",
  "productName": "Tuturu Timer",
  "version": "3.0.0",
  "description": "A sleek and minimalist desktop timer application built with Electron and React. This cross-platform app combines modern UI aesthetics with essential timing functionality",
  "main": "./out/main/index.js",
  "keywords": [
    "electron",
    "react",
    "timer",
    "desktop-app",
    "productivity"
