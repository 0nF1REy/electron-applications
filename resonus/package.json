{
  "name": "resonus",
  "productName": "Resonus",
  "version": "4.0.0",
  "description": "Resonus: Where Sound (Sonus) meets Resonance. An immersive local music player for your personal collection, featuring a dynamic audio visualizer and a modern, urban-inspired design",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "build": "electron-builder"
  },
  "keywords": [
    "electron",
    "react",
    "desktop-app",
    "typescript"
  ],
  "author": {
    "name": "Alan Ryan da Silva Domingues",
    "email": "alanryan619@gmail.com"
  },
  "homepage": "https://github.com/0nF1REy/electron-applications",
  "repository": {
    "type": "git",
    "url": "https://github.com/0nF1REy/electron-applications.git"
  },
  "bugs": {
    "url": "https://github.com/0nF1REy/electron-applications/issues"
  },
  "license": "MIT",
  "build": {
    "appId": "com.onf1rey.resonus",
    "productName": "Resonus",
    "files": [
      "**/*"
    ],
    "directories": {
      "output": "dist"
    },
    "mac": {
      "icon": "assets/resonus-isotipo.icns",
      "category": "public.app-category.utilities"
    },
    "win": {
      "icon": "assets/resonus-isotipo.ico",
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "icon": "assets/resonus-isotipo.png",
      "target": [
        "AppImage"
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.9.0",
    "@electron-forge/maker-deb": "^7.9.0",
    "@electron-forge/maker-rpm": "^7.9.0",
    "@electron-forge/maker-squirrel": "^7.9.0",
    "@electron-forge/maker-zip": "^7.9.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.9.0",
    "@electron-forge/plugin-fuses": "^7.9.0",
    "@electron-forge/plugin-webpack": "^7.9.0",
    "@electron/fuses": "^1.8.0",
    "@svgr/webpack": "^8.1.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "css-loader": "^6.11.0",
    "electron": "38.1.2",
    "electron-builder": "^26.0.12",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.32.0",
    "fork-ts-checker-webpack-plugin": "^7.3.0",
    "node-loader": "^2.1.0",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "~4.5.4",
    "url-loader": "^4.1.1"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^10.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  }
}
